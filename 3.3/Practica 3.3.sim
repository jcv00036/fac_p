LDA 0100
MOV E A
LDA 0101
MOV D A
LDA 0102
MOV L A
LDA 0103
MOV H A

MOV A,D;
CPI 01;
JC ERROR;
JNZ NOSOLAPA;
MOV A,E;
CPI 04;
JC ERROR;

NOSOLAPA: MOV A H;
CMP D;
JC ERROR;
JNZ SINERRORES;
MOV A, L;
CMP E;
JC ERROR;
JZ ERROR;

SINERRORES:MVI C 00
IN 00
MOV B A
DCX D

RECORRER:INX D
    LDAX D
    CMP B
    JNZ NOCOINC
         INR C
    NOCOINC:MOV A D
    CMP H
    JNZ SEGUIR
        MOV A E
        CMP L
        JZ SALIR
SEGUIR:JMP RECORRER

SALIR:MOV A C
OUT 01
HLT
ERROR:MVI A FF
OUT 01
HLT